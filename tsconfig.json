{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "assets": ["src/assets/index"],
      "components": ["src/components/index"],
      "config/*": ["src/config/*"],
      "pages/*": ["src/pages/*"],
      "redux/*": ["src/redux/*"],
      "router/*": ["src/router"],
      "utils/*": ["src/utils/*"]
    }
  },
  "exclude": ["node_modules"]
}
